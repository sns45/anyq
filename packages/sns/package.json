{
  "name": "@anyq/sns",
  "version": "0.1.0",
  "description": "AWS SNS adapter for anyq",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anyq/core": "workspace:*",
    "@aws-sdk/client-sns": "^3.650.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@anyq/core": "workspace:*"
  },
  "keywords": ["anyq", "queue", "sns", "aws", "pub-sub", "fan-out"],
  "license": "MIT",
  "author": "Shantanu Sharma",
  "repository": {
    "type": "git",
    "url": "https://github.com/sns45/anyq",
    "directory": "packages/sns"
  },
  "homepage": "https://github.com/sns45/anyq#readme",
  "bugs": {
    "url": "https://github.com/sns45/anyq/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
