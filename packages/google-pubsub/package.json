{
  "name": "@anyq/google-pubsub",
  "version": "0.1.0",
  "description": "Google Cloud Pub/Sub adapter for anyq",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anyq/core": "workspace:*",
    "@google-cloud/pubsub": "^4.7.2"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@anyq/core": "workspace:*"
  },
  "keywords": [
    "anyq",
    "queue",
    "google",
    "pubsub",
    "gcp",
    "message-queue"
  ],
  "license": "MIT",
  "author": "Shantanu Sharma",
  "repository": {
    "type": "git",
    "url": "https://github.com/sns45/anyq",
    "directory": "packages/google-pubsub"
  },
  "homepage": "https://github.com/sns45/anyq#readme",
  "bugs": {
    "url": "https://github.com/sns45/anyq/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
