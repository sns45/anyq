{
  "name": "@anyq/memory",
  "version": "0.1.0",
  "description": "In-memory queue adapter for anyq - for testing and development",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "dev": "bun build ./src/index.ts --outdir ./dist --target bun --watch",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@anyq/core": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "queue",
    "message-queue",
    "memory",
    "testing",
    "anyq"
  ],
  "license": "MIT",
  "author": "Shantanu Sharma",
  "repository": {
    "type": "git",
    "url": "https://github.com/sns45/anyq",
    "directory": "packages/memory"
  },
  "homepage": "https://github.com/sns45/anyq#readme",
  "bugs": {
    "url": "https://github.com/sns45/anyq/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
