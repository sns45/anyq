{
  "name": "@anyq/core",
  "version": "0.1.1",
  "description": "Core interfaces and utilities for anyq - Universal Queue Library",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware/index.js",
      "types": "./dist/middleware/index.d.ts"
    },
    "./serialization": {
      "import": "./dist/serialization/index.js",
      "types": "./dist/serialization/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "dev": "bun build ./src/index.ts --outdir ./dist --target bun --watch",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "queue",
    "message-queue",
    "typescript",
    "kafka",
    "rabbitmq",
    "sqs",
    "redis",
    "nats",
    "pubsub"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sns45/anyq",
    "directory": "packages/core"
  }
}
