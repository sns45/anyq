{
  "name": "@anyq/nats",
  "version": "0.0.1",
  "description": "NATS JetStream adapter for anyq",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anyq/core": "workspace:*",
    "nats": "^2.28.2"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@anyq/core": "workspace:*"
  },
  "keywords": [
    "anyq",
    "queue",
    "nats",
    "jetstream",
    "message-queue",
    "pubsub"
